<%= form_tag '/pictures', method: :post, class: "dropzone", id: "media-dropzone" do %>
  <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>" />
  <div class="fallback">
    <%= file_field_tag "media", multiple: true%>
  </div>

<% end %>
<br>
<%= button_tag 'Upload file', class: 'btn btn-small btn-primary btn-remove-file', id: 'submit-all' %>
