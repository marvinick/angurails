<%= simple_form_for @place, :html => {:class => 'form-signin' } do |f| %>
  <%= f.error_notification %>
  <h3 class='text-center'>New Place</h3>
  <%= f.input :name, label: false, placeholder: "street and/or city", input_html: {class: "form-control"} %>
  <%= f.input :state, label: false, placeholder: "state", input_html: {class: "form-control"} %>
  <%= f.button :submit, class:"btn btn-info btn-lg" %>
<% end %>
